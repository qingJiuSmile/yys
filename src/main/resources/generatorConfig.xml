<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包-->
    <classPathEntry location="D:/postgresql-42.2.5.jar" />
    <context id="pg" targetRuntime="MyBatis3">
        <!--去掉自动生成的注解-->
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <jdbcConnection driverClass="org.postgresql.Driver"
                        connectionURL="jdbc:postgresql://localhost:5432/postgres"
                        userId="postgres"
                        password="root">
        </jdbcConnection>

        <javaModelGenerator targetPackage="cn.qingjiu.yys.entity" targetProject="D:/J-USER/desktop7/my/yys/src/main/java">
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.mapper"  targetProject="D:/J-USER/desktop7/my/yys/src/main/resources">
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="cn.qingjiu.yys.mapper"  targetProject="D:/J-USER/desktop7/my/\yys/src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table tableName="login_user——"  enableCountByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false"
               selectByExampleQueryId="false" enableSelectByExample="false">
        </table>

    </context>
</generatorConfiguration>